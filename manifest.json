{
  "manifest_version": 2,
  "name": "Dominion Enhancer",
  "version": "0.1",
  "icons": {
    "128": "chancellor-2.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://dominion.games/*", "http://dominion.games/*"
      ],
      "css": ["de.css"],
      "js": ["jquery-3.5.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "chancellor-2.png",
    "default_popup": "popup.html"
  }
}